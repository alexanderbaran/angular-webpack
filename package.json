{
  "name": "angular-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cleanup": "rimraf dist/src dist/node_modules src/app/**/*.json src/app/**/*.ngstyle.ts src/app/**/*.ngfactory.ts",
    "scsstocss": "node-sass src/app/ -o src/app/ && postcss --use autoprefixer --autoprefixer.browsers \"last 10 version\" --replace src/app/**/*.css && replace .scss .css src/app -r --include=*.component.ts",
    "csstoscss": "replace .css .scss src/app -r --include=*.component.ts && rimraf src/app/**/*.css",
    "ngc": "ngc -p tsconfig.aot.json && ngc -p tsconfig.aot.json",
    "dev": "webpack-dev-server --config webpack.config.dev.js",
    "build": "rimraf dist && webpack --config webpack.config.prod.js",
    "build:aot": "rimraf dist && npm run scsstocss && npm run ngc && webpack --config webpack.config.aot.js && npm run csstoscss && npm run cleanup"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^2.4.2",
    "@angular/compiler": "^2.4.2",
    "@angular/core": "^2.4.2",
    "@angular/forms": "^2.4.2",
    "@angular/http": "^2.4.2",
    "@angular/platform-browser": "^2.4.2",
    "@angular/platform-browser-dynamic": "^2.4.2",
    "@angular/router": "^3.4.2",
    "core-js": "^2.4.1",
    "rxjs": "^5.0.3",
    "zone.js": "^0.7.4"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.2",
    "@angular/platform-server": "^2.4.2",
    "@types/node": "^6.0.58",
    "angular2-router-loader": "^0.3.4",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.26.0",
    "node-sass": "^4.2.0",
    "postcss-cli": "^2.6.0",
    "postcss-loader": "^1.2.1",
    "raw-loader": "^0.5.1",
    "replace": "^0.3.0",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "typescript": "2.0.3",
    "webpack": "2.2.0-rc.3",
    "webpack-dev-server": "2.2.0-rc.0",
    "webpack-merge": "^2.3.1"
  }
}
